{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["react-native", "react-native-vector-icons"],
    "jsx": "react",
    "strict": true,
    "target": "es2017",
    "lib": ["ES2017", "dom"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
